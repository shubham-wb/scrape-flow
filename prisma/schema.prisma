generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Workflow {
  id          String   @id @default(cuid())
  userId      String
  name        String
  description String
  definition  String
  status      String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  // user cannot have two workflows with the same name and id 
  @@unique([name, userId])
}
